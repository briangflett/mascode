<?xml version="1.0"?>
<extension key="mascode" type="module">
  <file>mascode</file>
  <name>mascode</name>
  <description>This extension is used by Mananagement Advisory Services (MAS) to customize their implementation of CiviCRM.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Brian Flett</author>
    <email>brian.flett@masadvise.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/briangflett/mascode</url>
    <url desc="Documentation">https://github.com/briangflett/mascode/blob/master/README.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <downloadUrl>https://github.com/briangflett/mascode/archive/refs/tags/v{version}.tar.gz</downloadUrl>
  <releaseUrl>https://raw.githubusercontent.com/briangflett/mascode/master/releases.json</releaseUrl>
  <releaseDate>2025-10-19</releaseDate>
  <version>1.0.5</version>
  <develStage>production</develStage>
  <compatibility>
    <ver>6.1</ver>
  </compatibility>
  <requires>
    <ext>org.civicoop.civirules</ext>
  </requires>
  <comments>This is work in progress</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Mascode</namespace>
    <format>25.10.1</format>
    <angularModule>crmMascode</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>setting-admin@1.0.1</mixin>
  </mixins>
</extension>
